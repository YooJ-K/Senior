
digraph G1{
    edge [arrowhead=none, arrowtail=none];
    node [shape = circle];
    rankdir="LR";
    
    a -> b[label="p"];
    a->e[label="q"];
    b->e[label="r"];
    b->c[label="r"];
    b->d[label="r"];
    c->d[label="p"];

}

