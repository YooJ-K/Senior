    edge [arrowhead=none, arrowtail=none];
    node [shape = circle];
    rankdir="LR";
    
    a -> b[label="p"];
    a->d[label="q"];
    b->d[label="r"];

}

digraph G3{
    edge [arrowhead=none, arrowtail=none];
    node [shape = circle];
    rankdir="LR";
    
    a->b[label="r"];
    a->c[label="r"];
    b->c[label="p"];
    c->d[label="q"];
    c->e[label="p"];

}

digraph G4{
    edge [arrowhead=none, arrowtail=none];
    node [shape = circle];
    rankdir="LR";
    
    a->e[label="q"];
    a->c[label="p"];
    e->c[label="p"];
    c->d[label="p"];

}

digraph apriori_1{
    layout=neato;
    edge [arrowhead=none, arrowtail=none];
    node [shape = circle];
    
    a[pos="0,0!"];
    b[pos="1,0!"];
    c[pos="2,0!"];
    d[pos="3,0!"];
    e[pos="4,0!"];
    
}

